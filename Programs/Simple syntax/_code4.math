начало программы
{
	а, б.
	
	вывод "Введите число а:".
	ввод а.
	вывод "Введите число б:".
	ввод б.
	
	сумма = Сумма(а, б).
	ср_квад = Сред вадрат(а, б).
	ср_геом = СредГеом(а, б).
	
	вывод "Сумма чисел: ", сумма.
	вывод "Среднее квадратичное чисел: ", ср_квад.
	вывод "Среднее геометрическое: ", ср_геом.
}

возвр Сумма(а, б)
{
	вернуть а + б.
}

возвр СредКвадрат(а, б)
{
	вернуть корень((а^2 + б^2)/2).
}

возвр СредГеом(a, b)
{
	вернуть корень (a  *  b).
}