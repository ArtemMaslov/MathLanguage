начало программы
{
	а, б, в.
	ввод а, б, в.
	
	если а == 0, то
	{
		Если б == 0 и в == 0, то
			вывод беск. число решений.
		Иначе если б == 0 и с != 0, то
			вывод Решений нет.
		вывод -в/б.
	}
	иначе
	{
		Д = б^2 - 4*а*в.
		
		если Д < 0, то
			вывод решений нет.
		иначе если Д == 0, то
			вывод -б/(2*а).
		иначе
		{
			х1 = (-б + корень(Д))/(2*а).
			х2 = (-б - корень(Д))/(2*а).
			вывод х1, х2.
		}
	}
}
		