Начало программы
{
	массив[21] = {123, 12, 53, 64, 1,8 ,34 ,64, 29, 53, 21, 53,61, 82, 91, 40, 10, 54, 13, 53, 8}.
	
	для всех ст от 0 до 20 по 1:
		вывод массив[ст].
		
	СортировкаПузырьком(массив, 21).
	
	для всех ст от 0 до 20 по 1:
		вывод массив[ст].
}

СортировкаПузырьком(массив, размер)
{
	для всех ст1 от 0 до размер - 1 по 1:
		для всех ст2 от ст1 + 1 до размер по 1:
			если массив[ст1] > массив[ст2]:
				элем = массив[ст1].
				массив[ст1] = массив[ст2].
				массив[ст2] = элем.
}

